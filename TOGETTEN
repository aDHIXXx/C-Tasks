#include <stdio.h>
void main()
{int n,i,total; int array[100]; float avg;
scanf("%d",&n);
for(i=0;i<n;i++)
   scanf("%d",&array[i]);
avg=0;
total=0;
for(i=0;i<n;i++)
  total=total+array[i];
avg=(total*1.00)/n;
i=0;
while(avg < 9.5000)
 { 
   i=i+1;
   total=total+10;
   avg=(total*1.00 )/(n+i);
 }
 printf("%d\n",i);
}
